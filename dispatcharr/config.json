{
  "name": "Dispatcharr",
  "slug": "dispatcharr",
  "description": "Proxy M3U/EPG & HDHomeRun out",
  "version": "0.4.1",
  "startup": "services",
  "boot": "auto",
  "image": "ghcr.io/lemehdi0/dispatcharr:latest"
  "url": "https://github.com/LeMehdi0/Dispatcharr"
  "arch": [
    "aarch64",
    "armv7",
    "amd64"
  ],
  "ingress": true,
  "ingress_port": 9191,
  "ingress_entry": "/",
  "panel_icon": "mdi:television-guide",
  "ports": {
    "9191/tcp": 9191,
    "8001/tcp": 8001
  },
  "map": [
    "share:rw"
  ],
  "options": {
    "TZ": "Africa/Casablanca",
    "PUID": 1000,
    "PGID": 1000,
    "CELERY_WORKER_CONCURRENCY": 2,
    "UWSGI_PROCESSES": 2,
    "UWSGI_GEVENT": 50,
    "DISPATCHARR_SKIP_IP_LOOKUP": true
  },
  "schema": {
    "TZ": "str?",
    "PUID": "int?",
    "PGID": "int?",
    "CELERY_WORKER_CONCURRENCY": "int?",
    "UWSGI_PROCESSES": "int?",
    "UWSGI_GEVENT": "int?",
    "DISPATCHARR_SKIP_IP_LOOKUP": "bool?"
  }
}
